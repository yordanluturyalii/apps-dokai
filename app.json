{
  "expo": {
    "name": "evia-apps",
    "slug": "evia-apps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dokai-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.555019552152-u2mnu72d15032td8e6m1o4ngvamt3g3m"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dokai-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.eviaapps",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/dokai-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/dokai-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Evia Apps to access your camera"
        }
      ],
      [
        "expo-image-picker",
        {
          "photoPermission": "Allow Evia Apps to access your photos"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "65755089-238a-46ec-87c6-94ab2904e954"
      }
    }
  }
}
